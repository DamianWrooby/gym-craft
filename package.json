{
    "name": "gym-craft",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "prisma generate && vite build",
        "migrate": "npx prisma migrate deploy",
        "build-and-migrate": "npm run build && npm run migrate",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "lint": "eslint .",
        "format-check": "prettier --plugin-search-dir . --check .",
        "format": "prettier --plugin-search-dir . --write .",
        "postinstall": "prisma generate",
        "test": "vitest run",
        "test:ui": "vitest --ui",
        "test:watch": "vitest",
        "deploy": "npm run format-check && npm run lint && npm run test && npm run build-and-migrate"
    },
    "devDependencies": {
        "@floating-ui/dom": "^1.7.2",
        "@netlify/edge-functions": "^2.16.0",
        "@skeletonlabs/skeleton": "^2.11.0",
        "@skeletonlabs/tw-plugin": "^0.3.1",
        "@sveltejs/adapter-auto": "^2.1.1",
        "@sveltejs/adapter-netlify": "^1.0.5",
        "@sveltejs/kit": "^1.30.4",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.16",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/svelte": "^5.2.8",
        "@types/bcrypt": "^5.0.2",
        "@types/lodash": "^4.17.20",
        "@types/node": "^20.19.9",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitest/ui": "^3.2.4",
        "autoprefixer": "^10.4.21",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-svelte": "^2.46.1",
        "jsdom": "^26.1.0",
        "postcss": "^8.5.6",
        "postcss-load-config": "^5.1.0",
        "prettier": "^3.6.2",
        "prettier-plugin-prisma": "^5.0.0",
        "prettier-plugin-svelte": "^3.4.0",
        "prisma": "^5.22.0",
        "svelte": "^4.2.20",
        "svelte-check": "^3.8.6",
        "svelte-feather-icons": "^4.2.0",
        "tailwindcss": "^3.4.17",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "vite": "^4.5.14",
        "vite-plugin-tailwind-purgecss": "^0.2.1",
        "vitest": "^3.2.4"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "type": "module",
    "dependencies": {
        "@prisma/client": "^5.22.0",
        "@types/dom-view-transitions": "^1.0.6",
        "@types/nodemailer": "^6.4.17",
        "@types/nodemailer-mailgun-transport": "^1.4.6",
        "await-to-js": "^3.0.0",
        "bcrypt": "^5.1.1",
        "html2pdf.js": "0.9.3",
        "lodash": "^4.17.21",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.10.1",
        "nodemailer-mailgun-transport": "^2.1.5",
        "openai": "^4.104.0"
    }
}
